{
  "name": "voragine-backoffice-backend",
  "version": "2.0.0",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.5.1",
    "cors": "^2.8.5",
    "express": "^4.7.1",
    "method-override": "^2.1.2",
    "pg": "^8.5.1"
  },
  "scripts": {
    "start": "node index.js",
    "serve": "nodemon index.js"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "es6": true,
      "browser": true
    },
    "rules": {
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-unused-vars": [
        "warn"
      ],
      "no-var": [
        "off"
      ],
      "one-var": [
        "off"
      ],
      "eslint linebreak-style": [
        0,
        "error",
        "windows"
      ]
    }
  },
  "devDependencies": {
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.7"
  }
}
